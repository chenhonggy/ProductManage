<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.launcher.productmanage.main" >

    //获取mac地址的权限
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    //获取网络数据权限
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:theme="@style/MyTheme"
            android:label="@string/home"
            android:name="com.launcher.productmanage.Activity.Product_HomeActivity"
            android:icon="@drawable/ic_launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:theme="@style/MyTheme"
            android:name="com.launcher.productmanage.Activity.Product_DetailsActivity"
            android:label="@string/product_details"
            android:icon="@drawable/ic_launcher"
            />
    </application>

</manifest>
